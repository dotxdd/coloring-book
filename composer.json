{
    "name": "dotxdd/coloring-book",
    "description": "PHP library for generating coloring book pages from images",
    "type": "library",
    "license": "MIT",
    "version": "1.0.4",
    "authors": [
        {
            "name": "Gabriel Moskwa",
            "email": "gabriel.moskwa@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "intervention/image": "^3.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dotxdd\\ColoringBook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dotxdd\\ColoringBook\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
